{
    "reddit": {
        "user_agent": "ticker_scraper"
    },
    "stocktwits": {
        "request_token_url": "https://api.stocktwits.com/api/2/oauth/token",
        "authorize_url": "https://api.stocktwits.com/api/2/oauth/authorize",
        "scope": "read,watch_lists,direct_messages,publish_messages,publish_watch_lists,follow_users"
    },
    "discord_exporter_cli": {
        "dll_file_directory": "DiscordChatExporter.Cli/DiscordChatExporter.Cli.dll",
        "output_directory": "data/DiscordMessages/${discord_group_name}/${date_timestamp}/${timestamp}"
    },
    "data_directory": {
        "ticker_list": "data/entire_ticker_list.csv",
        "ticker_ignore": "data/ticker_ignore.csv",
        "stocktwits_trending": "data/stocktwits_trending/${datetime}/trending.csv",
        "discord_ticker_mentions": "data/DiscordMessages/discord_ticker_mentions/${discord_group_name}/${date_timestamp}/${timestamp}"
    }
}